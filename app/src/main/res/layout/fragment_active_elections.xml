<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="wrap_content"
  tools:context=".ActiveElectionsFragment">

  <ScrollView
    android:id="@+id/active_elections"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <RelativeLayout
      android:id="@+id/content_container"
      android:layout_width="wrap_content"
      android:layout_height="812dp">


      <RelativeLayout
        android:id="@+id/page_active_elections_ek1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp">

        <View
          android:id="@+id/_bg__active_elections_ek2"
          android:layout_width="375dp"
          android:layout_height="812dp"
          android:layout_marginStart="0dp"
          android:layout_marginLeft="0dp"
          android:layout_marginTop="0dp"
          android:background="@drawable/_bg__active_elections_ek2_shape" />

        <TextView
          android:id="@+id/active_elections_ek3"
          android:layout_width="175dp"
          android:layout_height="38dp"
          android:layout_gravity="center_horizontal|center"
          android:layout_marginLeft="99dp"
          android:layout_marginTop="21dp"
          android:fontFamily="FontAwesome"
          android:gravity="center"
          android:shadowColor="#704d4d"
          android:shadowDx="0"
          android:shadowDy="3"
          android:shadowRadius="2"
          android:text="@string/active_elections_ek3_string"
          android:textColor="@color/forgot_password_ek3_color"
          android:textSize="25sp" />


        <RelativeLayout
          android:id="@+id/group_52"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginStart="17dp"
          android:layout_marginTop="18dp">

          <View
            android:id="@+id/rectangle_1474_ek2"
            android:layout_width="47dp"
            android:layout_height="41dp"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            android:background="@drawable/rectangle_1474_ek2_shape" />


          <RelativeLayout
            android:id="@+id/backward_arrow_ek20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginTop="12dp">

            <ImageView
              android:id="@+id/path_10_ek22"
              android:layout_width="16dp"
              android:layout_height="16dp"
              android:layout_marginStart="0dp"
              android:layout_marginLeft="0dp"
              android:layout_marginTop="0dp"
              android:contentDescription="path_10_ek22"
              android:src="@drawable/path_10_ek22" />

          </RelativeLayout>

        </RelativeLayout>

        <ImageView
          android:id="@+id/undraw_google_docs_jf93_ek1"
          android:layout_width="371dp"
          android:layout_height="275dp"
          android:layout_marginStart="5dp"
          android:layout_marginLeft="5dp"
          android:layout_marginTop="54dp"
          android:contentDescription="undraw_google_docs_jf93_ek1"
          android:src="@drawable/undraw_google_docs_jf93_ek1" />


        <RelativeLayout
          android:id="@+id/group_49"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginStart="0dp"
          android:layout_marginTop="65dp">

          <ImageView
            android:id="@+id/path_238_ek21"
            android:layout_width="206dp"
            android:layout_height="22dp"
            android:layout_marginStart="-9dp"
            android:layout_marginLeft="-9dp"
            android:layout_marginTop="-6dp"
            android:contentDescription="path_238_ek21"
            android:src="@drawable/path_238_ek21" />

          <ImageView
            android:id="@+id/path_239_ek21"
            android:layout_width="206dp"
            android:layout_height="22dp"
            android:layout_marginStart="179dp"
            android:layout_marginLeft="179dp"
            android:layout_marginTop="-6dp"
            android:contentDescription="path_239_ek21"
            android:src="@drawable/path_239_ek21" />

        </RelativeLayout>

        <View
          android:id="@+id/rectangle_169"
          android:layout_width="309dp"
          android:layout_height="218dp"
          android:layout_marginStart="32dp"
          android:layout_marginLeft="32dp"
          android:layout_marginTop="315dp"
          android:alpha="0.45882352941176"
          android:background="@drawable/rectangle_169_shape" />

      </RelativeLayout>


      <ImageView
        android:id="@+id/undraw_file_sync_ot38"
        android:layout_width="271dp"
        android:layout_height="184dp"
        android:layout_marginStart="-3633dp"
        android:layout_marginLeft="-3633dp"
        android:layout_marginTop="-663dp"
        android:contentDescription="undraw_file_sync_ot38"
        android:src="@drawable/undraw_file_sync_ot38" />

      <ImageView
        android:id="@+id/undraw_empty_xct9_ek1"
        android:layout_width="264dp"
        android:layout_height="200dp"
        android:layout_marginStart="-3048dp"
        android:layout_marginLeft="-3048dp"
        android:layout_marginTop="-674dp"
        android:contentDescription="undraw_empty_xct9_ek1"
        android:src="@drawable/undraw_empty_xct9_ek1" />

    </RelativeLayout>

  </ScrollView>
</ScrollView>